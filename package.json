{
  "name": "magicgreen11ty",
  "version": "3.2.8",
  "description": "Magic Green 11ty",
  "scripts": {
    "build": "npm run sass && eleventy",
    "debug": "DEBUG=* eleventy",
    "sass": "sass --style=compressed src/scss/main.scss src/_includes/css/main.css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "start": "npm-run-all sass --parallel watch:*",
    "start:clean": "rm -rf _site/ && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realmagicgreen/magicgreen11ty.git"
  },
  "keywords": [
    "frontend",
    "ssg",
    "eleventy",
    "environment-conscious-architecture",
    "environment-friendly-web-solution"
  ],
  "author": "toybreaker",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/realmagicgreen/magicgreen11ty/issues"
  },
  "homepage": "https://github.com/realmagicgreen/magicgreen11ty#readme",
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@sindresorhus/slugify": "^2.1.0",
    "eleventy-plugin-date": "^1.0.0",
    "eleventy-plugin-reading-time": "0.0.1",
    "markdown-it": "^12.1.0",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "sass": "^1.43.4"
  }
}
